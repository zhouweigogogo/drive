QTableWidget{   
    background: transparent;    
    border:none;
}
QHeaderView{
    background:transparent;        
}
QTableWidget::item
{
    border:none;
    border-top:1px solid rgb(135,206,250) ;	/*用户管理右侧用户显示底部线条颜色*/
}
